{% extends 'base.html.twig' %}

{% block title %} Nouvelle spécialité {% endblock %}

{% block body %}

<h1>Ajout d'une nouvelle spécialité</h1>

{% include 'elements/arborescence.html.twig' %}

<div>
    {{ form_start(form) }}
    
	{{ form_row(form.etablissement) }}
	{{ form_row(form.service) }}
	{{ form_row(form.code_logistique) }}
	{{ form_row(form.adulte) }}
	{{ form_row(form.pediatrie) }}
    
    {{ form_rest(form) }}
    {{ form_errors(form) }}
    {{ form_end(form) }}
    
</div>
	  
{% endblock %}